# Backend environment variables for TeleMed-Pro
# Copy this file to backend/.env and fill in real values

# MongoDB connection
MONGO_URL=mongodb://localhost:27017
DB_NAME=telemed_pro

# CORS (comma-separated list)
CORS_ORIGINS=http://localhost:3000

# LLM provider key for emergent integrations
EMERGENT_LLM_KEY=your-emergent-llm-api-key

